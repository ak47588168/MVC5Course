@model DateTime?

@if (Model.HasValue)
{
    @Html.TextBox("", Model.Value.ToString("yyyy-MM-dd"), new { @class = "" })
}
else
{
    @Html.TextBox("", null, new { @class = "" })
}

<script type="text/javascript">
    $(function () {
        $("#@Html.Id("")").datepicker({ dateFormat: "yy-MM-dd" });
    })
</script>